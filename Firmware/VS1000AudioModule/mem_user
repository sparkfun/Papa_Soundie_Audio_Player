MEMORY {
    page 0:
 	ram_prog: origin = 0x0050, length = 0x07b0
# 	ram2_prog: origin = 0x1000, length = 0x0400
    page 1:
# actually 0x1f9d..0x2400
	bss_x:    origin = 0x1fa0, length = 0x460
    page 2:
# actually 0x1b32..0x1c00
	bss_y:    origin = 0x1b32, length = 0xce
}

CORE {
	frequency = 12MHz
}

